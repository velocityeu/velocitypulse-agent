{
  "name": "velocitypulse-agent",
  "version": "1.1.0",
  "description": "Network monitoring agent for VelocityPulse SaaS platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run copy-static",
    "copy-static": "node --input-type=commonjs -e \"require('fs').cpSync('src/ui/public','dist/ui/public',{recursive:true})\"",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3",
    "@types/express": "^5.0.6",
    "@types/node-ssdp": "^4.0.5",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "multicast-dns": "^7.2.5",
    "net-snmp": "^3.26.1",
    "node-ssdp": "^4.0.1",
    "oui": "^13.1.3",
    "oui-data": "^1.1.516",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "tsx": "^4.19.2",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/velocityeu/velocitypulse-agent.git"
  },
  "keywords": [
    "network",
    "monitoring",
    "agent",
    "velocitypulse",
    "saas"
  ],
  "author": "Velocity EU",
  "license": "MIT",
  "overrides": {
    "ip": "2.0.1"
  }
}
